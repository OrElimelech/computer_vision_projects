{
  "data_set_conf": {
    "annotations_path": "car_plates_dataset/annotations",
    "images_path": "car_plates_dataset/images",
    "data_dimension": 400
  },
  "training_conf": {
    "enable_training": true,
    "validation_ratio": 0.02,
    "epoch_num": 15,
    "batch_size": 16,
    "object_existence_loss_weight": 0.5,
    "bounding_box_loss_weight": 3,
    "optimizer_learning_rate": 0.0001
  },
  "model_conf": {
    "model_type": "VGG16",
    "model_top_config": {
      "Flatten_Layer": {
        "initializing_parameters": {},
        "layer_type": "Flatten"
      },
      "dense_1_existence": {
        "initializing_parameters": {
          "units": 5,
          "activation": "sigmoid"
        },
        "input_layer": "Flatten_Layer",
        "layer_type": "Dense"
      }
    }
  },
  "call_back_conf": {
    "ReduceLROnPlateau": {
      "patience": 5,
      "verbose": 1,
      "factor": 0.00005
    }
  }
}